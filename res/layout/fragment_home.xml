<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home" >

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/profilepiximageView"
        android:layout_marginLeft="52dp"
        android:layout_toRightOf="@+id/profilepiximageView"
        android:src="@drawable/sam_timeline"
         />

    <TextView
        android:id="@+id/profiletextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/profilepiximageView"
        android:layout_below="@+id/profilepiximageView"
        android:layout_marginTop="23dp"
        android:text="Profile" />

    <TextView
        android:id="@+id/gendertextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/profiletextView"
        android:layout_alignBottom="@+id/profiletextView"
        android:layout_alignLeft="@+id/imageView2"
        android:text="Timeline" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/gendertextView"
        android:layout_alignLeft="@+id/imageView3"
        android:text="Email" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/gendertextView"
        android:layout_alignTop="@+id/displayimageView"
        android:src="@drawable/sam_chat" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignTop="@+id/imageView5"
        android:src="@drawable/sam_notification" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/displaynametextView"
        android:layout_alignBottom="@+id/displaynametextView"
        android:layout_alignLeft="@+id/imageView5"
        android:text="Chat" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView4"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignLeft="@+id/imageView6"
        android:text="Notification" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignTop="@+id/imageView7"
        android:src="@drawable/sam_twitter" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignTop="@+id/imageView8"
        android:src="@drawable/sam_linkedin" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignLeft="@+id/imageView8"
        android:text="Twitter" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignLeft="@+id/imageView9"
        android:text="Linkedin" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/profiletextView"
        android:layout_marginLeft="44dp"
        android:layout_toRightOf="@+id/gendertextView"
        android:onClick="startEmail"
        android:src="@drawable/sam_email" />

    <TextView
        android:id="@+id/displaynametextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/displayimageView"
        android:layout_below="@+id/displayimageView"
        android:layout_marginTop="35dp"
        android:text="Events" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/displaynametextView"
        android:layout_below="@+id/displaynametextView"
        android:layout_marginTop="44dp"
        android:src="@drawable/sam_facebook"
        android:onClick="startFacebook" />

    <ImageView
        android:id="@+id/profilepiximageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/sam_profile" />

    <ImageView
        android:id="@+id/displayimageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/profiletextView"
        android:layout_below="@+id/profiletextView"
        android:layout_marginTop="27dp"
        android:onClick="startCalendar"
        android:src="@drawable/sam_calender" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView7"
        android:layout_below="@+id/imageView7"
        android:layout_marginTop="27dp"
        android:text="Facebook" />

</RelativeLayout>